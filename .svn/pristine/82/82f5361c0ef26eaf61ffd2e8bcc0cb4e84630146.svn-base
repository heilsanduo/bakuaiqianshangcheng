<component src="layout/index.html" title="八块钱商城" header></component>

<div id="seller_layout" class="layout-view">
    <div class="register">
        <div class="register_header">
            <div class="register_text">
                <div class="register-text-now">注册</div>
                <p class="register-text-new">新用户欢迎您~</p>
            </div>
            <div class="register_main">
                <form action="post">
                    <div class="register_main_phone">
                        <h5 class="register_phone">手机号码</h5>
                        <div class="register_phone_num">
                            <input id="phone_num" type="tel" placeholder="请输入手机号码">
                        </div>

                    </div>
                    <div class="register_main_code">
                        <h5 class="register_code">验证码</h5>
                        <div class="register_code_num">
                            <input id="phone_code" type="number" placeholder="请输入验证码">
                            <button id="send_code" class="register_send_code">获取验证码</button>
                        </div>

                    </div>
                    <div class="register_main_password">
                        <h5 class="register_password">设置密码</h5>
                        <div class="register_password_num">
                            <input id="password" type="password" placeholder="请设置密码">
                        </div>

                    </div>
                    <div class="register_main_dbpassword">
                        <h5 class="register_dbpassword">确认密码</h5>
                        <div class="register_dbpassword_num">
                            <input id="dbPassword" type="password" placeholder="请确认密码">
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <div class="register_footer">
            <div class="register_footer_text">
                <div id="choose" class="register_footer_icon">
                    <img src="../../images/register_unselected.png" alt="">
                </div>
                <div class="register_footer_agree">
                    <div class="register_agree_read">我已阅读并同意</div>
                    <div class="register_agree_file">
                        <<注册协议>>
                    </div>
                </div>
                <div class="register_footer_login">已有账号登录</div>
            </div>
            <div id="btn" class="register_footer_now">
                注册并登录
            </div>
        </div>
    </div>
</div>

<component src="footer/index.html"></component>