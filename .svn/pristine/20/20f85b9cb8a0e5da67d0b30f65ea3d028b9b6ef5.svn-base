<script>
	$(function () {
		//选项卡
		$("#mycollect-box-top div").click(function () {
			//点击当前的变高亮+排他
			$(this)
				.addClass("mycollect-active")
				.siblings("div")
				.removeClass("mycollect-active");
			//内容跟着出来
			$("#mycollect-box-bottom .option")
				.eq($(this).index())
				.css("display", "block")
				.siblings()
				.css("display", "none");
		});
	});

</script>
