<script>
    $(function () {
        //选项卡
        $("#myGroupBuy-box-top div").click(function () {
            //点击当前的变高亮+排他
            $(this)
                .addClass("myGroupBuy-active")
                .siblings()
                .removeClass("myGroupBuy-active");
            //内容跟着出来
            $("#myGroupBuy-box-bottom .myGroupBuy_option")
                .eq($(this).index())
                .css("display", "block")
                .siblings()
                .css("display", "none");
        });
    });

</script>