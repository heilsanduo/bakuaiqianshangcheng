@import "../../../styles/common.less";
@import "../../../styles/main.less";
// 底部
#seller_map_layout{
  height: 100%;
  overflow: hidden;
  display:flex;
  flex-direction: column;
}
.seller-map-back{
  z-index: 100;
  width: 0.64rem;
  height: 0.64rem;
  background-color:#fff;
  border-radius: 50%;
  position: absolute;
  left:0.24rem;
  top: 0.32rem;
  .bgSet(0.36rem 0.36rem);
  background-image:url("../images/seller_home_header_back_black_icon.png");
  .hover-class;
}
.seller-map-content{
  width: 100%;
  flex:auto;
  background-color:pink;
}
.seller-map-footer{
  fleX: none;
  width: 100%;
  height: 3.56rem;
  background-color:#fff;
  display:flex;
  flex-direction: column;
  padding: 0.48rem 0.32rem 0.32rem 0.32rem;
  justify-content: space-between;
}
.seller-map-box{
  width: 100%;
  height: 1.32rem;
  display:flex;
  align-items: center;
  padding:0 0.32rem;
}
.seller-map-icon{
  width: 1.6rem;
  height: 1.06rem;
  margin-right: 0.32rem;
  border:1px solid rgba(245,245,245,1);
  border-radius: 0.04rem;
  flex:none;
  .imgAll;
}
.seller-map-warp{
  flex:auto;
  display:flex;
  flex-direction: column;
  justify-content: space-between;
  overflow: hidden;
}
.seller-map-name{
  height: 0.44rem;
  font-size: 0.32rem;
  font-weight:800;
  color:rgba(51,51,51,1);
  .fontBreak(1, 0.44rem);
}
.seller-map-address{
  height: 0.72rem;
  font-size: 0.26rem;
  font-weight:400;
  color:rgba(51,51,51,1);
  .fontBreak(2, 0.36rem);
}

.seller-map-btn{
  .button-default;
  height: 0.84rem;
}










