@import "../../../styles/common.less";
.search_index{
    width:100%;
    height:100%;
  .search_new{
      width:100%;
      height:100%;
     .search_new_header{
        width:100%;
        height:0.44rem;
        display:flex;
        padding:0.48rem 0.32rem 0.32rem 0.32rem;
        box-sizing:border-box;
        .search_new_title{
        flex:auto;
        height:0.44rem; 
        position: relative; 
        >h5{
            width:100%;
            font-size:0.32rem;
            font-weight:800;
            line-height:0.44rem;
            color:rgba(51,51,51,1);
        }
        .search_new_icons{
            width:1.24rem;
            height:0.08rem;
            background:rgba(255,222,69,1);
            position: absolute;
           bottom:0.04rem;
            left:0;
            z-index: -2;
        }

        }
        .search_remove{
            width: 0.44rem;
            height: 0.44rem;
            background-image:url("../images/search_remove.png");
            .bgSet();
         }
     } 
     .search_new_detail{
        width:100%;
        height:100%;
        overflow: hidden;
        >li{
        height:0.4rem;
        float:left;
        font-size:0.28rem;
        font-weight:400;
        line-height:0.4rem;
        color:rgba(51,51,51,1);
        padding-right:0.32rem;
        box-sizing:border-box;
        }
     }
     
  }
}