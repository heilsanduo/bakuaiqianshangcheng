@import "../../../styles/common.less";
@import "../../../styles/main.less";
// 底部
#seller_detail_layout{
  padding: 0 0.32rem;
}
.seller-detail-content{
  width: 100%;
  overflow: hidden;
  padding: 0 0.32rem;
  background-color:#fff;
  box-shadow:0 0.08rem 0.32rem rgba(0,0,0,0.08);
  border-radius: 0.2rem;
  margin: 0.32rem auto;
}
.seller-detail-box{
  width: 100%;
  height: 1.98rem;
  display:flex;
  border-bottom: 1px solid rgba(245,245,245,1);
  padding: 0.32rem 0;
}


.seller-detail-icon{
  width: 2rem;
  height: 1.34rem;
  margin-right: 0.32rem;
  border:1px solid rgba(245,245,245,1);
  border-radius: 0.04rem;
  flex:none;
  .imgAll;
}
.seller-detail-warp{
  flex:auto;
  display:flex;
  flex-direction: column;
  justify-content: space-between;
  overflow: hidden;
  padding: 0.19rem 0;
}
.seller-detail-name{
  height: 0.44rem;
  font-size: 0.32rem;
  font-weight:800;
  color:rgba(51,51,51,1);
  .fontBreak(1, 0.44rem);
}
.seller-detail-tell{
  height: 0.36rem;
  font-size: 0.26rem;
  font-weight:400;
  color:rgba(51,51,51,1);
  display:flex;
  align-items: center;
}
.seller-detail-tell-icon{
  width: 0.34rem;
  height: 0.34rem;
  background-image: url("../images/seller_home_tell_icon.png");
  .bgSet();
}
.seller-detail-message{
  font-size: 0.28rem;
  font-weight:400;
  line-height: 0.42rem;
  color:rgba(51,51,51,1);
  padding: 0.32rem 0;
}









