    // orderConfirm_detail
    .orderConfirm_detail {
        width: 6.86rem;
        height: 2.92rem;
        background: rgba(255, 255, 255, 1);
        box-shadow: 0 0.08rem 0.32rem rgba(0, 0, 0, 0.08);
        border-radius: 0.2rem 0.2rem 0 0;
    }

