    .orderConfirm_items_main {
        width: 100%;
        height: 2.32rem;
        display: flex;
        padding: 0.32rem 0.32rem 0.32rem 0;
        margin-bottom: 0.32rem;
        box-sizing: border-box;
    }

    .items_main_img {
        width: 2rem;
        height: 2rem;
        background: rgba(245, 125, 125, 0);
        border-radius: 0.12rem;

        >img {
            width: 2rem;
            height: 2rem;
        }
    }

    .items_main_text {
        flex: auto;
        padding-left: 0.36rem;
