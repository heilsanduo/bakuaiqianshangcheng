    //  leave massage
    .leave_massage {
        width: 6.86rem;
        height: 1.82rem;
        background: rgba(255, 255, 255, 1);
        box-shadow: 0 0.08rem 0.32rem rgba(0, 0, 0, 0.08);
        border-radius: 0.16rem;
        display: flex;
        // align-items: center;
        padding: 0 0.32rem;
        box-sizing: border-box;
    }

    .massage_leaver {
        flex: auto;
        font-size: 0.28rem;
        font-weight: 800;
        color: rgba(51, 51, 51, 1);
        margin-right: 0.32rem;
        padding-top: 0.32rem;
        box-sizing: border-box;
    }

    .massage_detail {
        width: 4.78rem;
        height: 100%;
    }

    .massage_text {
        width: 4.78rem;
        height: auto;
        position: relative;
        padding-top: 0.32rem;
        box-sizing: border-box;

        .wishContent {
            resize: none;
            width: 4.78rem;
            min-height: 1.23rem;
            font-size: 0.28rem;
            font-weight: 400;
            line-height: 0.42rem;
            color: rgba(153, 153, 153, 1);
            outline: none;
            border: 0px;
        }

        .wordsNum {
            position: absolute;
            bottom: 0.12rem;
            right: 0rem;
            color: #B3B3B3;
            font-size: 0.1rem;
        }
    }


    // orderConfirm_footer
    .orderConfirm_footer {
        width: 100%;
        height: 0.98rem;
        background: rgba(255, 255, 255, 1);
        box-shadow: 0 -0.04rem 0.08rem rgba(0, 0, 0, 0.04);
        position: fixed;
        bottom: 0;
        display: flex;
        align-items: center;
        padding-right: 0.16rem;
        box-sizing: border-box;
    }

    .orderConfirm_total {
