        color: rgba(51, 51, 51, 1);
        text-align: right;
        padding-right: 0.48rem;
        box-sizing: border-box;
    }

    .orderConfirm_btn {
        width: 2.1rem;
        height: 0.74rem;
        font-size: 0.26rem;
        font-weight: 800;
        color: rgba(51, 51, 51, 1);
        text-align: center;
        background: rgba(255, 222, 69, 1);
        border-radius: 0.08rem;
        line-height: 0.74rem;

    }

}

// 弹窗内容区
.coupons_popup {
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.6);
    display: none;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 2;

    .coupons_popup-mask {
        width: 100%;
        height: 8.14rem;
    }

    .coupons_popup_content {
        width: 100%;
        height: 3.92rem;
        background: rgba(255, 255, 255, 1);
        border-radius: 0.2rem 0.2rem 0 0;
        z-index: 1;
        position: absolute;
        bottom: 0;
        padding: 0.76rem 0.46rem 0 0.32rem;
        box-sizing: border-box;
    }

    .coupons_popup_content-close {
        width: 0.44rem;
        height: 0.44rem;
        .bgSet();
        background-image: url("../images/close_black_icon.png");
        position: absolute;
        top: 0.32rem;
        right: 0.32rem;
    }

    .coupons_popup_list {
        width: 100%;
        height: 100%;

        >li {
            width: 100%;
            height: 1.02rem;
            font-weight: 400;
            line-height: 1.02rem;
            font-size: 0.28rem;
            border-bottom: 0.01rem solid rgb(238, 236, 236);
        }
    }

    .coupons_popup_unused {
        width: 100%;
        height: 1.02rem;
        display: flex;
        align-items: center;
    }

    .coupons_popup_text {
        flex: auto;
        font-weight: 400;
        font-size: 0.28rem;
        color: rgba(241, 90, 83, 1);
    }

    .coupons_popup_btn {
        width: 0.36rem;
        height: 0.36rem;
        border: 0.01rem dashed #ccc;
    }
}